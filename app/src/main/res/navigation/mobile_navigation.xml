<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_game">

    <fragment
        android:id="@+id/navigation_game"
        android:name="edu.gatech.schoolmark.ui.game.GameFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_game" />

    <fragment
        android:id="@+id/navigation_mygame"
        android:name="edu.gatech.schoolmark.ui.mygame.MyGameFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_mygame" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="edu.gatech.schoolmark.ui.profile.ProfileFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_profile" />
</navigation>